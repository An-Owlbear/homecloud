general:
  secureCookies: "true"
share:
  allowRegistration: "false"
oauth:
  allowRegistration: "true"
  ignoreTotp: "true"
  disablePassword: "false"
  oidc-enabled: "true"
  oidc-discoveryUri: "{{.OAuthIssuerUrl}}/.well-known/openid-configuration"
  oidc-signOut: "false"
  oidc-usernameClaim: ""
  oidc-rolePath: ""
  oidc-roleGeneralAccess: ""
  oidc-roleAdminAccess: ""
  oidc-clientId: "{{.OAuthClientID}}"
  oidc-clientSecret: "{{.OAuthClientSecret}}"
initUser:
  enabled: "false"
  username: admin
  email: admin@example.com
  password: my-secure-password
  isAdmin: true
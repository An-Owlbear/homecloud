package templates

import kratos "github.com/ory/kratos-client-go"

templ Login(flow kratos.UiContainer) {
    @Layout("login") {
        <form action={ templ.SafeURL(flow.Action) } method={ getUiContainerMethod(flow) }>
            for _, node := range flow.Nodes {
                @Node(node)
            }
        </form>
    }
}